; This file is for unifying the coding style for different editors and IDEs.
; More information at http://editorconfig.org

root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.py]
indent_size = 4

[*.php]
indent_size = 4

[*.json]
indent_size = 4

[*.go]
indent_style = tab

[*.bat]
end_of_line = crlf

[*.md]
trim_trailing_whitespace = false
end_of_line = crlf
